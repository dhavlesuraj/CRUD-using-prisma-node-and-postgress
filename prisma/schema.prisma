// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user{
  id Int @id @default(autoincrement())
  name String?
  email String @unique
  password String?
  post post[]
  comment comment[]
  created_at DateTime 
}
model post{
  id Int @id @default(autoincrement())
  user user @relation(fields: [user_id],references: [id], onDelete: Cascade)
  user_id Int
  title String
  description String
  comment_count Int @default(0)
  created_at DateTime 
  comment comment[]
}

model comment{
  id Int @id @default(autoincrement())
  post post @relation(fields: [post_id],references: [id],onDelete: Cascade)
  post_id Int
  user user @relation(fields: [user_id],references: [id],onDelete: Cascade)
  user_id Int
  comment String
  created_at DateTime

}

model logg{
  id Int @id @default(autoincrement())
  request String?
  response String?
  route String?
  created_at DateTime
  taken_time Float
  more_data String?
  method String?
}

model concept{
  id Int @id @default(autoincrement())
  concept String 
  is_deleted Int @default(0)
  description String
}

model skill{
  id Int @id @default(autoincrement())
  subject_id Int
  skill_name String
  skill_definition String
  img_link String
  video_link String
  is_active String
  beginner_lower_sal Int
  beginner_upper_sal Int
  beginner_jobs String
  intermediate_lower_sal Int
  intermediate_upper_sal Int
  intermediate_jobs String
  advanced_lower_sal Int
  advanced_upper_sal Int
  advanced_jobs String
  created_at DateTime
  updated_at DateTime
  is_deleted Int
  skill_description String
  thumbnail_link String
  slug String
  meta_tag_c__c String?
  meta_description_c__c String?
  meta_skill_description__c String?
  
}



